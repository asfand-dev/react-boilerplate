{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "src"
        ],
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    }
  },
  "globals": {
    "fetch": false,
    "Headers": false,
    "btoa": false,
    "window": false,
    "document": false,
    "alert": false
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "max-len": [
      "error",
      { "code": 150 }
    ],
    "no-underscore-dangle": "off",
    "react/prop-types": "off",
    "react/jsx-filename-extension": "off",
    "no-nested-ternary": "off",
    "react/jsx-props-no-spreading": "off",
    "import/prefer-default-export": "off",
    "no-console": "off",
    "no-alert": "off",
    "no-param-reassign": "off",
    "jsx-a11y/no-static-element-interactions": "off",
    "jsx-a11y/click-events-have-key-events": "off",
    "no-plusplus": "off",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off",
    "react/no-array-index-key": "off",
    "no-continue": "off",
    "import/no-cycle": 0
  },
  "env": {
    "jest": true
  }
}